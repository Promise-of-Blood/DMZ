            motion:layout_constraintHorizontal_bias=".94"
            motion:layout_constraintVertical_bias=".3"/>

        <Constraint
            android:id="@id/lifestyle_letter_s"
            android:layout_width="0dp"
            motion:layout_constraintWidth_percent=".45"
            android:layout_height="0dp"
            motion:layout_constraintDimensionRatio=".8"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.01"
            motion:layout_constraintVertical_bias=".53"/>

        <Constraint
            android:id="@id/lifestyle_letter_t"
            android:layout_width="0dp"
            motion:layout_constraintWidth_percent=".27"
            android:layout_height="0dp"
            motion:layout_constraintDimensionRatio=".8"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias=".45"
            motion:layout_constraintVertical_bias=".55"/>

        <Constraint
            android:id="@id/lifestyle_letter_y"
            android:layout_width="0dp"
            motion:layout_constraintWidth_percent=".26"
            android:layout_height="0dp"
            motion:layout_constraintDimensionRatio=".8"
