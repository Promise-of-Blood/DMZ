    <!-- 기본적인 Transition 설정 -->


    <!-- 추가적으로 end에서 start로 다시 전환 설정 -->
    <Transition
        motion:constraintSetStart="@id/start"
        motion:constraintSetEnd="@id/end"
        motion:duration="7000"
        motion:autoTransition="animateToEnd">

        <KeyFrameSet>
            <KeyPosition
                motion:motionTarget="@id/lifestyle_bubble"
                motion:framePosition="25"
                motion:percentX="0.8" />

            <KeyPosition
                motion:motionTarget="@id/lifestyle_bubble"
                motion:framePosition="50"
                motion:percentX="0.2"
                motion:percentY="0.6"/>

            <KeyAttribute
                motion:motionTarget="@id/lifestyle_bubble"
                motion:framePosition="50"
                android:scaleX="1.3"
                android:scaleY="1.3"
                android:translationX="-50dp"
                android:rotation="-20"/>

            <KeyPosition
                motion:motionTarget="@id/lifestyle_bubble"
                motion:framePosition="75"
                motion:percentX="0.8"
                motion:percentY="0.8"/>

            <KeyAttribute
                motion:motionTarget="@id/lifestyle_bubble"
                motion:framePosition="90"
                android:scaleX="0.8"
                android:scaleY="0.8"
                android:rotation="0"/>


            <KeyAttribute
                motion:motionTarget="@id/lifestyle_flower"
                motion:framePosition="30"
                android:rotation="-5"/>

            <KeyAttribute
                motion:motionTarget="@id/lifestyle_flower"
                motion:framePosition="40"
                android:rotation="5"/>

            <KeyAttribute
                motion:motionTarget="@id/lifestyle_flower"
                motion:framePosition="50"
                android:rotation="0"/>

            <KeyAttribute
                motion:motionTarget="@id/lifestyle_flower"
                motion:framePosition="60"
                android:rotation="5"/>

            <KeyAttribute
                motion:motionTarget="@id/lifestyle_flower"
                motion:framePosition="70" />

            <KeyAttribute
                motion:motionTarget="@id/lifestyle_flower"
                motion:framePosition="90"
                android:rotation="-5"/>



            <KeyAttribute
                motion:motionTarget="@id/lifestyle_cloud"
                motion:framePosition="20"
                android:translationY="-310dp"/>

            <KeyAttribute
                motion:motionTarget="@id/lifestyle_cloud"
                motion:framePosition="30"
                android:translationY="-340dp"/>

            <KeyAttribute
                motion:motionTarget="@id/lifestyle_cloud"
                motion:framePosition="50"
                android:translationY="-320dp"/>

            <KeyAttribute
                motion:motionTarget="@id/lifestyle_cloud"
                motion:framePosition="70"
                android:translationY="-350dp"/>





        </KeyFrameSet>

    </Transition>

    <Transition
        motion:constraintSetStart="@id/start"
        motion:constraintSetEnd="@id/end"
        motion:duration="0"
        motion:autoTransition="animateToStart">

    </Transition>





</MotionScene>